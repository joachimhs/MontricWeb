<div {{bind-attr class=isSelected:selected:not-selected}} {{bind-attr width=textWidth}} {{bind-attr maxWidht=textWidth}}>
    {{#if isSelected}}
        <i class="fa fa-check-square-o"></i>
    {{else}}
        <i class="fa fa-square-o"></i>
    {{/if}}
    {{#if node.id}}
        {{node.id}}
